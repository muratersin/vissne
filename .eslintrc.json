{
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "vissne": true,
        "commonGlobal": true,
        "yall": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "import/no-dynamic-require": 0,
        "jsx-a11y/label-has-for": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "consistent-return": 0
    }
}
